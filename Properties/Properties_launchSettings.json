{
  "profiles": {
    "LuckyStars": {
      "commandName": "Project",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      },
      "applicationUrl": ""
    },
    "LuckyStars (Release)": {
      "commandName": "Project",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Production"
      }
    },
    "LuckyStars (Minimized)": {
      "commandName": "Project",
      "commandLineArgs": "--minimized",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Production"
      }
    },
    "LuckyStars (AutoStart)": {
      "commandName": "Project",
      "commandLineArgs": "--autostart",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Production"
      }
    }
  },
  "LuckyStarsSettings": {
    "AutoStartWithWindows": true,
    "AutoStartSettings": {
      "RegistryKeyPath": "SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Run",
      "RegistryKeyName": "LuckyStarsWallpaper",
      "StartupArguments": "--minimized --autostart"
    },
    "StartupBehavior": {
      "StartMinimized": false,
      "ShowSplashScreen": true,
      "RestorePreviousWallpaper": true,
      "CheckForUpdates": true
    },
    "SystemIntegration": {
      "EnableContextMenu": true,
      "EnableExplorerIntegration": true,
      "RegisterFileAssociations": true
    },
    "PerformanceSettings": {
      "UseHardwareAcceleration": true,
      "MaxMemoryUsageMB": 512,
      "OptimizeForBattery": true
    }
  }
}